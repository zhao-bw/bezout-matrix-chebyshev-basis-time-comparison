chebyshev_basis:= proc(x, i)
    local T0, T1, T_a, T_b, T_c, result, k;
    
    T0 := 1;  
    T1 := x;  
    
    result := [T0, T1];
    
    if i = 0 then;
        return 1;
    end if;

    if i = 1 then;
        return result;
    end if;

    T_b:= T0;  
    T_a:= T1;  
    
    for k from 2 to i do;
        T_c:= 2*x*T_a - T_b;  
        result := [op(result), T_c]; 
        T_b := T_a;  
        T_a := T_c;   
    end do;
    
    return result; 
end proc:


Bez_p:=proc(F::Matrix,G::Matrix)<Equation executable="true" style="2D Input" input-equation="" display=""></Equation>local i,j,m,n,C,r,a,b,B_p,C1,C2;
  
  n:=ColumnDimension(F)-1;
  m:=ColumnDimension(G)-1;
  r:=max(m,n);
  C:=Matrix(r,r);
  a:=Array(0..r,i-&gt; if i&lt;= n then F[1,i+1] else 0 end if);
  b:=Array(0..r,i-&gt; if i&lt;= m then G[1,i+1] else 0 end if);
  C:=Matrix(r,r,0);
  for j from 1 to r do;
    C[1,j]:=2*(a[r]*b[r-j]-a[r-j]*b[r]);
  end do;
  for i from 2 to r do;
   for j from i to r do;
    C[i,j]:=a[r-i+1]*b[r-j]-a[r-j]*b[r-i+1];
   end do;
  end do;
  
  for i from 2 to r-1 do;
   for j from i to r do;
    C1:= if j+1&lt;=r then C[i-1,j+1] else 0 end if;
    C2:= if i-2&gt;=1 then C[i-2,j]   else 0 end if;
    if j &lt;= n-2 then;
     C[i,j]:=2*C[i,j]+C1+C[i-1,j-1]-C2;
    elseif;
     C[i,j]:=2*C[i,j]+2*C1+C[i-1,j-1]-C2;
    end if;
   end do;
  end do;
  C[r,r]:=C[r,r]-1/2*(C[r-2,r]-C[r-1,r-1]);
  B_p:=Matrix(r,r,(i,j)-&gt;C[min(i,j),max(i,j)]);
  return B_p;
end proc:

buildU := proc(n)
    local T, U, k, poly, deg, j;
   
    T := Array(0..n):
    T[0] := 1;
    if n &gt;= 1 then;
        T[1] := x;
    end if;
    for k from 2 to n do;
        T[k] := expand(2*x*T[k-1] - T[k-2]);
    end do;

    U := Matrix(n+1, n+1, 0);

    for k from 0 to n do;
        poly := T[n-k];                
        deg  := degree(poly, x);
        for j from 0 to deg do;           
            U[k+1, n+1-deg+j] := coeff(poly, x, deg-j);
        end do;
    end do;

    return U;
end proc:



BezMatrix:=proc(f::Matrix,g::Matrix)
  local n, m, r, a, b, A, B, M, i, j;
   n := ColumnDimension(f) - 1;
   m := ColumnDimension(g) - 1;
   r := max(n, m);

   for i from 0 to n do;
       a[i] := f[1, n-i+1];
   end do;

   for i from 0 to m do;
       b[i] := g[1, m-i+1];
   end do;

   for i from 0 to r do;
       if i &gt; n then A[i] := 0 else A[i] := a[i] end if;
       if i &gt; m then B[i] := 0 else B[i] := b[i] end if;
   end do;
   M := Matrix(r, r, 0);

   for i from 1 to r do;
       for j from i to r do;
           M[i, j] := A[r - i + 1] * B[r - j] - B[r - i + 1] * A[r - j];
       end do;  
   end do;

    for i from 2 to r - 1 do;
        for j from i to r - 1 do;
            M[i, j] := M[i, j] + M[i - 1, j + 1];
        end do;
    end do;

    for i from 1 to r do;
        for j from i to r do;
            M[j, i] := M[i, j];
        end do;
    end do;

    return M;
end proc:

BezMatrix_trans:=proc(F::Matrix,G::Matrix) 


  local U,U_inv,U_t,U_inv_t,C_f,C_g,M_f,M_g,m,n,F_p,G_p,BezMatrix_p,BezMatrix_g,i,r;
  m       := ColumnDimension(F)-1;
  n       := ColumnDimension(G)-1;
  r       := max(m,n);
  U       := buildU(r);
  U_inv   := MatrixInverse(U);
  U_t     := Transpose(U);
  U_inv_t := Transpose(U_inv);
  C_f     := &lt;Matrix([0$(r-m)])|F&gt;;
  C_g     := &lt;Matrix([0$(r-n)])|G&gt;;
  F_p     := C_f.U;
  G_p     := C_g.U;


  BezMatrix_p := BezMatrix(F_p,G_p);
  BezMatrix_g := MatrixMatrixMultiply(SubMatrix(U_inv_t,[2..r+1],[2..r+1]),MatrixMatrixMultiply(BezMatrix_p,SubMatrix(U_inv,[2..r+1],[2..r+1])));
 
 return BezMatrix_g;
end proc:


